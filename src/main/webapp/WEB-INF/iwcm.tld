  <tag>
     <description>
     <![CDATA[
       i18n tag for text value

       supports pluralize
       search for keys in this order:
       Exact term,
       key + * + .value + *
       key + .X

       Example:
       key = components.ceptem.joboffer.requiredpractice

       components.ceptem.joboffer.requiredpractice.0 = Prax nieje pozadovana
       components.ceptem.joboffer.requiredpractice.1 = Jeden rok praxe
       components.ceptem.joboffer.requiredpractice.2.3.4 = {1} roky praxe
       components.ceptem.joboffer.requiredpractice.X = {1} rokov praxe

       <iwcm:text key="components.ceptem.joboffer.requiredpractice" param1="${value} pluralize="true"/>
     ]]>
     </description>
     <name>text</name>
     <tag-class>sk.iway.iwcm.tags.TextTag</tag-class>
     <body-content>empty</body-content>
     <attribute>
       <name>key</name>
       <required>true</required>
       <rtexprvalue>true</rtexprvalue>
     </attribute>
     <attribute>
       <name>param1</name>
       <required>false</required>
       <rtexprvalue>true</rtexprvalue>
     </attribute>
     <attribute>
       <name>param2</name>
       <required>false</required>
       <rtexprvalue>true</rtexprvalue>
     </attribute>
     <attribute>
       <name>param3</name>
       <required>false</required>
       <rtexprvalue>true</rtexprvalue>
     </attribute>
     <attribute>
       <name>defaultText</name>
       <required>false</required>
       <rtexprvalue>true</rtexprvalue>
     </attribute>
      <attribute>
       <name>pluralize</name>
       <required>false</required>
       <rtexprvalue>false</rtexprvalue>
       <type>java.lang.Boolean</type>
     </attribute>
     <attribute>
       <name>lng</name>
       <required>false</required>
       <rtexprvalue>true</rtexprvalue>
       <type>java.lang.String</type>
     </attribute>
